public class Aritmetica {
	private int num1;
	private int num2;
	

	public void setNum1(int num1) {
		this.num1 = num1;
	}
	
	public void setNum2(int num2) {
		this.num2 = num2;
	}
	
	public int soma() {
		return num1+num2;
	}
	
	public int produto() {
		return num1*num2;
	}
	
	public int diferenca() {
		return num1-num2;
	}
	
	public float quociente() {
		return (float)num1/(float)num2;
	}
}
