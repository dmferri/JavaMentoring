import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int num[] = new int[5];
		int maior = 0;
		int menor = 0;
		
		for (int i=0;i<5;i++) {
			System.out.printf("Numero " + (i+1) + ": ");
			num[i] = sc.nextInt();
		}
		
		for (int i=1;i<=5;i++) {
			if (num[i]>maior) {
				maior=num[i];
			}
			if (num[i]<menor) {
				menor=num[i];
			}
		}
		sc.close();
	}
}
